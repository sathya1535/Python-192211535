def fibonacci(n):
    fibonacci_sequence = []
   
    a, b = 0, 1
    for _ in range(n):
        fibonacci_sequence.append(a)
        
        a, b = b, a + b
    return fibonacci_sequence


n = int(input("Enter the value of n: "))
fib_numbers = fibonacci(n)
print("First", n, "Fibonacci numbers:", fib_numbers)

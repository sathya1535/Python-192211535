def fibonacci_sequence(up_to):
    
    fib_sequence = [0, 1]
  
    while fib_sequence[-1] + fib_sequence[-2] <= up_to:
        next_fib = fib_sequence[-1] + fib_sequence[-2]
        fib_sequence.append(next_fib)
    return fib_sequence


up_to_number = 50
fib_sequence = fibonacci_sequence(up_to_number)
print("Fibonacci sequence up to", up_to_number, ":", fib_sequence)
